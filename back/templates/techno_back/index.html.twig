{% extends 'base.html.twig' %}

{% block title %}Hello TechnoBackController!{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
    th{ padding-right: 15px; }
    td{ padding-right: 15px; }
</style>

<div class="example-wrapper">
    <h1>Techno list </h1>

    <table>
        <th>Techno</th><th>Domaine</th>
        {% for tech in technoList %}
            <tr>
                <td>{{ tech.name }}</td>
                <td>{{ tech.name }}</td>
                <td><a>Modifier</a>
                <td><a>Supprimer</a>
            </tr>
        {% endfor %}
    </table>
</div>
{% endblock %}
